{
  "name": "auth0r",
  "version": "0.0.11",
  "description": "Auth0r is a plug-n-play authentication manager",
  "main": "./lib/index.js",
  "types": "./lib/**/*.d.ts",
  "scripts": {
    "start": "./node_modules/.bin/tsc && node -r esm ./lib/index.js",
    "test": "./node_modules/.bin/mocha -r ts-node/register tests/**/*.spec.ts",
    "build": "./node_modules/.bin/tsc -d && ./install_package ../auth0r-example-webapp/node_modules/@pritch9/auth0r/",
    "lint": "./node_modules/.bin/tslint -p tsconfig.json",
    "publish": "./node_modules/.bin/tsc -d && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pritch9/Auth0r.git"
  },
  "keywords": [
    "authentication"
  ],
  "author": "William Pritchard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pritch9/Auth0r/issues"
  },
  "homepage": "https://github.com/pritch9/Auth0r#readme",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "sqlite3": "^4.1.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "knex": "^0.19.2",
    "node-rsa": "^1.0.5"
  }
}
